{
  "name": "panda-lyrics",
  "version": "0.1.2",
  "main": "./dist/main.js",
  "repository": "https://github.com/vbalien/PandaLyrics2.git",
  "author": "vbalien <webmaster@alien.moe>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn start:renderer\" \"yarn start:main\"",
    "start:main": "tsc -p tsconfig.json --outDir ./dist && electron --no-sandbox .",
    "start:renderer": "vite -c ./src/renderer/vite.config.ts",
    "build": "concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "tsc -p tsconfig.json",
    "build:renderer": "vite build -c ./src/renderer/vite.config.ts",
    "package": "yarn build && electron-builder build --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "PandaLyrics",
    "appId": "moe.alien.pandalyrics",
    "asar": true,
    "files": [
      "renderer",
      "main",
      "node_modules",
      "package.json",
      "assets"
    ],
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "vbalien",
      "repo": "PandaLyrics2"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "fast-xml-parser": "^4.0.9",
    "fix-path": "3.0.0",
    "font-list": "^1.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.5.2",
    "recoil": "^0.7.4",
    "sqlstring": "^2.3.3",
    "tinycolor2": "^1.4.2",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@emotion/babel-plugin": "^11.10.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.2",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sqlstring": "^2.3.0",
    "@types/tinycolor2": "^1.4.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "daisyui": "^2.20.0",
    "electron": "^19.0.10",
    "electron-builder": "^23.6.0",
    "esbuild": "^0.17.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.3.2",
    "react-refresh": "^0.14.0",
    "shx": "^0.3.3",
    "tailwindcss": "^3.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.4",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/*"
    ],
    "logLevel": "quiet"
  }
}
